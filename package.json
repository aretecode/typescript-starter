{
  "name": "typescript-starter",
  "version": "1.0.0",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "now-build": "tsc || true",
    "build": "tsc --outDir dist || true",
    "start": "node dist/index.js",
    "dev:strict": "STRICT_TYPING=true npx ts-node-dev --n src/index.ts",
    "dev:watch": "NODE_ENV=development ts-node-dev --respawn --transpileOnly src/index.ts",
    "dev": "NODE_ENV=development ts-node src/index.ts",
    "test:watch": "jest --config jest.config.js --watch"
  },
  "resolutions": {
    "node-fetch": "2.6.7"
  },
  "dependencies": {
    "express": "4.17.1",
    "node-fetch": "2.6.7"
  },
  "devDependencies": {
    "ts-jest": "24.1.0",
    "ts-node": "8.4.1",
    "jest": "24.9.0",
    "jest-cli": "24.9.0",
    "@types/jest": "24.0.21",
    "typescript": "3.7.0-beta",
    "@types/express": "4.17.1",
    "danger": "9.2.2",
    "prettier": "1.18.2",
    "eslint": "^6.6.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-typescript": "^0.14.0",
    "@typescript-eslint/parser": "2.5.0"
  },
  "license": "MIT",
  "description": "TypeScript Starter",
  "homepage": "https://github.com:aretecode/typescript-starter.git",
  "author": "James Wiens <james@jameswiens.dev> (https://jameswiens.dev)",
  "repository": {
    "type": "git",
    "url": "git@github.com:aretecode/typescript-starter.git"
  },
  "keywords": [
    "jameswiens",
    "james",
    "wiens",
    "dev",
    "aretecode",
    "typescript",
    "starter",
    "js",
    "boilerplate"
  ]
}
